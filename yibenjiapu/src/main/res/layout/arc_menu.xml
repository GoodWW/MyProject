<?xml version="1.0" encoding="UTF-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <com.xunman.yibenjiapu.menu.ArcLayout
        custom:toDegrees="360.0"
        custom:fromDegrees="270.0"
        custom:childSize="44dp"
        android:layout_centerInParent="true"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/item_layout"
        xmlns:custom="http://schemas.android.com/apk/res-auto"/>
    <FrameLayout
        android:layout_centerInParent="true"
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        android:id="@+id/control_layout">
        <com.xunman.yibenjiapu.utils.CircleImageView
            android:id="@+id/control_hint"
            android:layout_height="50dp"
            android:layout_width="50dp"
            android:src="@drawable/vase_right"
            android:duplicateParentState="true"
            android:layout_gravity="center"/>
    </FrameLayout>
</merge>